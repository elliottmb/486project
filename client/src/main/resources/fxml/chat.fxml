<?import java.lang.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.iastate.cs.theseguys.ui.ChatController">
	<children>
		<GridPane AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="642.0" minWidth="10.0" prefWidth="623.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="175.0" minWidth="0.0" prefWidth="137.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="526.0" minHeight="10.0" prefHeight="526.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="162.0" minHeight="0.0" prefHeight="34.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Button fx:id="submit" mnemonicParsing="false" onAction="#submitText" text="Submit" GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<ListView fx:id="chat" editable="false" prefHeight="200.0" prefWidth="200.0" />
				<TextField fx:id="input" GridPane.rowIndex="1" />
				<TextArea fx:id="users" editable="false" prefHeight="307.0" prefWidth="176.0" GridPane.columnIndex="1" />
            <Button fx:id="logout" onAction="#logout" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Logout" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
